<a href="/videos/new">Post a video</a> or <a href="/">Back to home</a>
<% @videos.each do |video| %>
  <div class="video">
    <h3><a href="/videos/<%= video.id %>"><%= video.title %></a> <small>(by: <%= video.author %>)</small><div class='time'><%= video.created_at %></div></h3>
      
      
    <iframe width="420" height="315" src="https://www.youtube.com/embed/<%= video.embed %>" frameborder="0" allowfullscreen></iframe>

  </div>
<% end %>

